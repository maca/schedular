<% form_for @event do |f| %>
  <%= f.error_messages %>
  
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%= f.label :dates %>
    <%= f.text_area :dates %>
  </p>
  
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>